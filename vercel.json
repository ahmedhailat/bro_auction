{
  "version": 2,
  "builds": [
    {
      "src": "build/web/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "FLUTTER_WEB_RENDERER": "canvaskit"  // or "html" if preferred
    }
  }
}
